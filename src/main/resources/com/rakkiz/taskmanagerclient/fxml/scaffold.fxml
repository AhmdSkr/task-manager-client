            <Cursor fx:constant="HAND"/>
        </cursor>
    </ImageView>

    <AnchorPane fx:id="content" minHeight="-Infinity" prefHeight="807.0" AnchorPane.leftAnchor="37.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
    </AnchorPane>
</AnchorPane>

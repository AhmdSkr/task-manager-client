    <ImageView fx:id="logo" fitHeight="50.0" fitWidth="207.0" onMouseClicked="#onTaskSackClick" pickOnBounds="true"
               preserveRatio="true" AnchorPane.leftAnchor="37.0" AnchorPane.topAnchor="40.0">
